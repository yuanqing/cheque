{
  "name": "cheque",
  "version": "0.2.1",
  "description": "Type checking, for when you only use JavaScript's Good Parts.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/cheque.git"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "recoil": "^0.1.0",
    "tape": "^3.0.3"
  },
  "scripts": {
    "lint": "node_modules/.bin/jshint --verbose index.js test/*.js",
    "test": "rm -rf coverage && node_modules/.bin/istanbul cover tape test/*.js"
